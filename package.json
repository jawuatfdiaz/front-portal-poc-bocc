{
  "name": "pocs-ntt",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ntt":"json -I -f src/config/config.json -e \"this.themeId='default'\" && npm run start",
    "start:davivienda": "json -I -f src/config/config.json -e \"this.themeId='davivienda'\" && npm run start",
    "start:sura":"json -I -f src/config/config.json -e \"this.themeId='sura'\" && npm run start",
    "build": "ng build",
    "build:ntt": "json -I -f src/config/config.json -e \"this.themeId='default'\" && npm run build",
    "build:davivienda": "json -I -f src/config/config.json -e \"this.themeId='davivienda'\" && npm run build",
    "build:sura": "json -I -f src/config/config.json -e \"this.themeId='sura'\" && npm run build",
    "test": "jest --verbose",
    "test:watch": "jest --watchAll",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:cov": "jest --coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/forms": "^15.2.10",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/router": "^15.2.10",
    "@azure/msal-angular": "^2.5.7",
    "@azure/msal-browser": "^2.37.0",
    "@azure/storage-blob": "^12.14.0",
    "@sweetalert2/ngx-sweetalert2": "^12.3.0",
    "angular-calendar": "^0.31.0",
    "bootstrap": "^5.3.0",
    "buffer": "^6.0.3",
    "date-fns": "^2.30.0",
    "json": "^11.0.0",
    "ng-multiselect-dropdown": "^0.3.9",
    "ngx-chips": "^3.0.0",
    "ngx-dropzone": "^2.3.0",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.10.8",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.10",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.10",
    "@angular/compiler-cli": "^15.2.10",
    "@angular/language-service": "^15.2.10",
    "@types/jest": "^29.5.12",
    "@types/node": "^12.20.55",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "ajv": "^8.12.0",
    "codelyzer": "^6.0.2",
    "eslint": "^8.33.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.0.4",
    "karma-coverage": "^2.0.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "protractor": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "~7.0.0",
    "tslint": "^6.1.3",
    "typescript": "~4.8.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "moduleDirectories": ["node_modules", "src"],
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ],
    "globalSetup": "jest-preset-angular/global-setup",
    "moduleNameMapper": {
      "^src/environments/environment$": "<rootDir>/src/app/mocks/environment.mock.ts"
    }
  }
}
